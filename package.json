{
  "name": "ai-art",
  "version": "2.1.5",
  "description": "Simple discord bot generating art from various AI models",
  "author": "Karafra",
  "private": true,
  "license": "Apache 2.0",
  "scripts": {
    "preinstall": "./scripts/install-system-dependencies.sh",
    "prebuild": "rimraf dist",
    "build": "npm run build:canvas && nest build",
    "build:canvas": "cd node_modules/canvas && npm run install && cd ../..",
    "postinstall": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "build:docs": "npx compodoc",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@discord-nestjs/common": "^3.4.2",
    "@discord-nestjs/core": "^3.6.0",
    "@karafra/nestjs-amqp": "github:karafra/nestjs-amqp",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^7.6.0",
    "@settlin/collage": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "apollo-server-express": "^3.10.0",
    "axios": "^0.27.2",
    "discord.js": "^13.8.1",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.3.0",
    "import": "^0.0.6",
    "mongodb": "^3.7.3",
    "nestjs-dynamic-providers": "^0.1.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "yaml-env-defaults": "^1.1.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}

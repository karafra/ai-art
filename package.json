{
  "name": "cc-bot",
  "version": "2.0.0",
  "description": "A general discord bot with multiple functionalities",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node -r ts-node/register -r tsconfig-paths/register build/main.js",
    "dev": "NODE_ENV=development nodemon",
    "lint": "eslint --ext ts .",
    "lint:fix": "npm run lint -- --fix",
    "npm:clear": "rm -rf $PWD/node_modules && rm $PWD/package-lock.json && npm cache verify && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {},
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^2.1.1",
    "@discordx/utilities": "^4.2.4",
    "@settlin/collage": "^2.0.1",
    "@types/underscore": "^1.11.4",
    "@types/urban-dictionary": "^3.0.0",
    "axios": "^0.26.1",
    "canvas": "^2.9.1",
    "chalk": "^4.1.1",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.11.1",
    "discord.js": "^13.6.0",
    "discordx": "^9.5.3",
    "dotenv": "^16.0.0",
    "minecraft-server-util": "^4.1.2",
    "mongodb": "^4.5.0",
    "opusscript": "^0.0.8",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.12.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.4.0",
    "underscore": "^1.13.3",
    "urban-dictionary": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.26",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/main.ts",
    "ext": "ts, js"
  }
}
